  "bugs": {
    "url": "https://github.com/johnshivers3/pigout/issues"
  },
  "homepage": "https://github.com/johnshivers3/pigout#readme",
  "dependencies": {
    "react-geocode": "^0.2.3",
    "redux-devtools-extension": "^2.13.9"
  },
  "devDependencies": {
    "concurrently": "^6.3.0"
  }
}
